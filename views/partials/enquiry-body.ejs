        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-sm-8">
                                <h4>
                                Enquiry Log</h4>
                            </div>
                            <div class="col-sm-4">
                            <a href="/addenquiry" class="btn btn-lg" style="float:right ;"><i class="fa fa-plus fa-fw"></i>Add New Enquiry</a>
                        </div>
                        </div>
                            
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        
                        <table width="100%" class="table table-bordered table-hover table-striped" id="enquiry_table">
                            <thead>
                                <tr>
                                        
                                    <th>#</th>
                                    
                                    <th>Project</th>
                                    <th class="hidden">Project Location</th>
                                    <th>Customer</th>
                                    <th>Qty</th>
                                    <th>Amount</th>
                                    <th>Date</th>
                                    <th>Sales Person</th>
                                    <th >Status</th>
                                    
                                </tr>
                            </thead>
                            
                            <tbody class="table-bordered">
                                
                                    <% items.forEach(function(item){ %>
                                        
                                        <tr>
                                                <td><% if(item.status=="Booked"){ %>
                                                    <a href="/viewenquiry/<%= item.job_ref %>" class="btn btn-success"><%= item.job_ref %></a>
                                                    <% } else if(item.status=="Quoted") { %>
                                                        <a href="/viewenquiry/<%= item.job_ref %>" class="btn btn-info"><%= item.job_ref %></a>
                                                            
                                                            <% } else if(item.status=="Lost") { %> 
                                                                <a href="/viewenquiry/<%= item.job_ref %>" class="btn btn-default"><%= item.job_ref %></a>
                                                                <% } else if(item.status=="Processing") { %> 
                                                                    <a href="/viewenquiry/<%= item.job_ref %>" class="btn btn-outline btn-info"><%= item.job_ref %></a>
                                                                <% } %>
                                                </td>
                                                <td><%= item.project %></td>
                                                <td class="hidden"><%= item.location %></td>
                                                <td><%= item.customer %></td>
                                                <td><%= item.qty %></td> 
                                                <td><%= currencyFormatter.format(item.amount, { code: 'INR' }); %></td>
                                                <td><%= moment(item.reci_date).format('DD-MMM-YY'); %></td>
                                                <td><%= item.sales_per %></td>
                                                <td ><%= item.status %></td>
                                                  
                                                       
                                            </tr>
                                            <% }); %>
                            </tbody>
                            
                        </table>
                        
                            
                        <!-- /.table-responsive -->

                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>
