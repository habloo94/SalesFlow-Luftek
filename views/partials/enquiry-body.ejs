        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-sm-8">
                                <h4>
                                Enquiry Log</h4>
                            </div>
                            <div class="col-sm-4">
                            <a href="/addenquiry" class="btn btn-lg" style="float:right ;"><i class="fa fa-plus fa-fw"></i>Add New Enquiry</a>
                        </div>
                        </div>
                            
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        
                        <table width="100%" class="table table-bordered table-hover" id="enquiry_table">
                            <thead>
                                <tr>
                                        
                                    <th>#</th>
                                    
                                    <th>Project</th>
                                    <th class="hidden">Project Location</th>
                                    <th>Customer</th>
                                    <th>Qty</th>
                                    <th>Amount</th>
                                    <th>Date</th>
                                    <th>Sales Person</th>
                                    <th >Status</th>
                                    <th ><i class=" fa fa-gear"></i></th>
                                </tr>
                            </thead>
                            
                            <tbody class="table-bordered">
                                
                                    <% items.forEach(function(item){ %>
                                        <tr>
                                                <td><%= item.job_ref %></td>
                                                <td><%= item.project %></td>
                                                <td class="hidden"><%= item.location %></td>
                                                <td><%= item.customer %></td>
                                                <td><%= item.qty %></td> 
                                                <td><%= currencyFormatter.format(item.amount, { code: 'INR' }); %></td>
                                                <td><%= moment(item.reci_date).format('DD-MMM-YY'); %></td>
                                                <td><%= item.sales_per %></td>
                                                <td ><%= item.status %></td>
                                                <% if(item.status=="Booked"){ %>
                                                            <td >
                                                                    <a href="/editenquiry/<%= item.job_ref %>" class="btn btn-primary btn-circle"><i class="fa fa-edit"></i></a>
                                                                        <a href="/viewenquiry/<%= item.job_ref %>" class="btn btn-info btn-circle"><i class="fa fa-file-text"></i></a>
                                                                        <a href="/revenquiry/<%= item.job_ref %>" class="btn btn-warning btn-circle"><i class="fa fa-pencil"></i></a>
                                                                        <a href="/booking/<%= item.job_ref %>" class="btn btn-success btn-circle disabled "><i class="fa fa-check"></i></a>
                                                                    </td>
                                                                 <% } else { %>
                                                                    <td >
                                                                            <a href="/editenquiry/<%= item.job_ref %>" class="btn btn-primary btn-circle"><i class="fa fa-edit"></i></a>
                                                                                <a href="/viewenquiry/<%= item.job_ref %>" class="btn btn-info btn-circle"><i class="fa fa-file-text"></i></a>
                                                                                <a href="/revenquiry/<%= item.job_ref %>" class="btn btn-warning btn-circle"><i class="fa fa-pencil"></i></a>
                                                                                <a href="/booking/<%= item.job_ref %>" class="btn btn-danger btn-circle "><i class="fa fa-check"></i></a>
                                                                            </td>    
                                                                    <% } %>      
                                                       
                                            </tr>
                                            <% }); %>
                            </tbody>
                            
                        </table>
                        
                            
                        <!-- /.table-responsive -->

                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>
