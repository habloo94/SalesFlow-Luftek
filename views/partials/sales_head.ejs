            function myFunction() {
                var x = document.getElementById("bvalue_select").value;
                if(x=="Total"){
                    document.getElementById("bvalue").innerHTML = "Net Booking Value:  <%= currencyFormatter.format(salest, { code: 'INR' }); %>" ;
                } else if(x=="Azeem"){
                    document.getElementById("bvalue").innerHTML = "Net Booking Value:  <%= currencyFormatter.format(salesa, { code: 'INR' }); %>" ;
                } else if(x=="Suresh M"){
                    document.getElementById("bvalue").innerHTML = "Net Booking Value:  <%= currencyFormatter.format(saless, { code: 'INR' }); %>" ;
                } else if(x=="Factory"){
                    document.getElementById("bvalue").innerHTML = "Net Booking Value:  <%= currencyFormatter.format(salesf, { code: 'INR' }); %>" ;
                }
                
            }
