<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1></h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-primary">

                <div class="panel-heading">
                    <div class="row">
                        <div class="col-sm-10">
                            <h3>Enquiry #
                                <%= item[0].job_ref %>
                            </h3>
                        </div>
                        <div class="col-sm-2">
                            <a href="/enquiry" class="btn btn-lg btn-primary" style="float:right ;">
                                <i class="fa fa-times fa-fw"></i>
                            </a>
                            <a href="/enquiry" class="btn btn-lg btn-primary" style="float:right ;">
                                <i class="fa fa-print fa-fw"></i>
                            </a>

                        </div>


                    </div>
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <h3 class="page-header">Project Details</h3>
                    <div class="row">
                        <div class="col-lg-4">
                            <h4>
                                <i>Project Name :</i>
                                <%= item[0].project %>
                            </h4>
                        </div>
                        <div class="col-lg-4">
                            <h4>
                                <i>Location :</i>
                                <%= item[0].location %>
                            </h4>
                        </div>
                        <div class="col-lg-4">
                            <h4>
                                <i>Type :</i>
                                <%= item[0].project_type %>
                            </h4>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-4">
                            <h4>
                                <i>Consultant :</i>
                                <%= item[0].consultant %>
                            </h4>
                        </div>
                        <div class="col-lg-4">
                            <h4>
                                <i>Contractor :</i>
                                <%= item[0].contractor %>
                            </h4>
                        </div>
                        <div class="col-lg-4">
                            <h4>
                                <i>Client :</i>
                                <%= item[0].client %>
                            </h4>
                        </div>
                    </div>
                    <h3 class="page-header">Customer Details</h3>
                    <div class="row">
                        <div class="col-lg-4">
                            <h4>
                                <i>Customer Type :</i>
                                <%= item[0].customer_type %>
                            </h4>
                        </div>
                        <div class="col-lg-4">
                            <h4>
                                <i>Customer Name :</i>
                                <%= item[0].customer %>
                            </h4>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-4">
                            <h4>
                                <i>Contact Person :</i>
                                <%= item[0].contact_per %>
                            </h4>
                        </div>
                        <div class="col-lg-4">
                            <h4>
                                <i>Contact No. :</i>
                                <%= item[0].contact_num %>
                            </h4>
                        </div>
                    </div>
                    <h3 class="page-header">Quotation</h3>
                    <div class="row">
                        <div class="col-lg-12">
                            <h4>
                                <i>Product :</i>
                                <%= item[0].product %>
                            </h4>
                            <table width="100%" class="table table-bordered table-hover">
                                <thead>
                                    <th>Revision No.</th>
                                    <th>Recieved Date</th>
                                    <th>Sent Date</th>
                                    <th>Qty</th>
                                    <th>Amount</th>
                                    <th>Note</th>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>R0</td>
                                        <td>
                                            <%= item[0].reci_date %>
                                        </td>
                                        <td>
                                            <%= item[0].sent_date %>
                                        </td>
                                        <td>
                                            <%= item[0].qty %>
                                        </td>
                                        <td>
                                            <%= currencyFormatter.format(item[0].amount, { code: 'INR' }); %>
                                        </td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td>R1</td>
                                        <td></td>
                                        <td>
                                            <%= item[0].r1date %>
                                        </td>
                                        <td></td>
                                        <td>
                                            <%= currencyFormatter.format(item[0].r1amt, { code: 'INR' }); %>
                                        </td>
                                        <td>
                                            <%= item[0].r1note %>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>R2</td>
                                        <td></td>
                                        <td>
                                            <%= item[0].r2date %>
                                        </td>
                                        <td></td>
                                        <td>
                                            <%= currencyFormatter.format(item[0].r2amt, { code: 'INR' }); %>
                                        </td>
                                        <td>
                                            <%= item[0].r2note %>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>R2</td>
                                        <td></td>
                                        <td>
                                            <%= item[0].r2date %>
                                        </td>
                                        <td></td>
                                        <td>
                                            <%= currencyFormatter.format(item[0].r2amt, { code: 'INR' }); %>
                                        </td>
                                        <td>
                                            <%= item[0].r2note %>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>R3</td>
                                        <td></td>
                                        <td>
                                            <%= item[0].r3date %>
                                        </td>
                                        <td></td>
                                        <td>
                                            <%= currencyFormatter.format(item[0].r3amt, { code: 'INR' }); %>
                                        </td>
                                        <td>
                                            <%= item[0].r3note %>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>R4</td>
                                        <td></td>
                                        <td>
                                            <%= item[0].r4date %>
                                        </td>
                                        <td></td>
                                        <td>
                                            <%= currencyFormatter.format(item[0].r4amt, { code: 'INR' }); %>
                                        </td>
                                        <td>
                                            <%= item[0].r4note %>
                                        </td>
                                    </tr>
                                    <tr>
                                            <td>R5</td>
                                            <td></td>
                                            <td>
                                                <%= item[0].r5date %>
                                            </td>
                                            <td></td>
                                            <td>
                                                <%= currencyFormatter.format(item[0].r5amt, { code: 'INR' }); %>
                                            </td>
                                            <td>
                                                <%= item[0].r5note %>
                                            </td>
                                        </tr>
                                </tbody>

                            </table>
                        </div>
                    </div>
                    <h4 class="page-header"></h4>
                    <div class="row">
                        <div class="col-lg-12">
                            <h3>Project Status :
                                <%= item[0].status %>
                            </h3>
                        </div>
                    </div>
                    <h4 class="page-header"></h4>
                    <div class="row">
                        <div class="col-lg-6">
                            <h4>
                                <i>Quoted by :</i>
                                <%= item[0].app_engg %>
                            </h4>
                        </div>

                        <div class="col-lg-6">
                            <h4>
                                <i>Sales Call Created by :</i>
                                <%= item[0].sales_per %>
                            </h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>