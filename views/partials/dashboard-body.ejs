<div id="page-wrapper" style="
margin-left: 0px;">
    <div class="row">
        <div class="col-lg-12">
                
            <h2 class="page-header">Welcome! Mr.<%= user.user_id %></h2>
            
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <div class="row">
            <% if(user.user_data.office == "Factory" && user.user_data.control == "child" ){ %> <%} else {%>
            <div class="col-lg-4 col-md-2">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-3">
                                    <i class="fa fa-tasks fa-fw fa-5x"></i>
                            </div>
                            <a href="" style="color: ghostwhite;" data-toggle="modal" data-target="#myModal">
                            <div class="col-xs-9 text-right">
                                <div class="huge">
                                <%= items %>
                                </div>
                                <div>Pending Enquiries</div>
                            </div>
                            </a>
                            <!-- Modal -->
                            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog" style="width : 80%;">
                                    <div class="modal-content" >
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                            <h4 class="modal-title" id="myModalLabel">Modal title</h4>
                                        </div>
                                        <div class="modal-body" style="height: auto;" >
                                            <table width="auto" class="table table-bordered table-hover table-responsive">
                                                <thead>
                                                    <tr>
                                                        <th>Job#</th>
                                                        <th>Project</th>
                                                        <th>Customer</th>
                                                        <th>Date</th>
                                                        <th>Qty</th>
                                                        <th>Sales</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                        <% plists.forEach(function(plist){ %>
                                                    <tr class="warning">
                                                        <td><a href="/viewenquiry/<%= plist.job_ref %>" class="btn btn-outline btn-info"><%= plist.job_ref %></a></td>
                                                        <td><%= plist.project %></td>
                                                        <td><%= plist.customer %></td>
                                                        <td><%= moment(plist.reci_date).format('DD-MMM-YY'); %></td>
                                                        <td><%= plist.qty %></td>
                                                        <td><%= plist.sales_per %></td>
                                                    </tr>
                                                    <% }); %>
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                        </div>
                                    </div>
                                    <!-- /.modal-content -->
                                </div>
                                <!-- /.modal-dialog -->
                            </div>
                            <!-- /.modal -->
                        </div>
                    </div>
                    <a href="/enquiry">
                        <div class="panel-footer">
                                <span class="pull-left">View Enquiry List</span>
                            <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                            <div class="clearfix"></div>
                        </div>
                    </a>
                </div>
            </div> <%} %>
            <div class="col-lg-4 col-md-2">
                <div class="panel panel-green">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-3">
                                <i class="fa fa-qrcode fa-fw fa-5x"></i>
                            </div>
                            <a href="" style="color: ghostwhite;" data-toggle="modal" data-target="#myModal1">
                            <div class="col-xs-9 text-right">
                                <div class="huge"><%= sales %></div>
                                <div>New Orders</div>
                            </div>
                            </a>
                            <!-- Modal -->
                            <div class="modal fade" id="myModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog" style="width : 80%;">
                                    <div class="modal-content" >
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                            <h4 class="modal-title" id="myModalLabel">Modal title</h4>
                                        </div>
                                        <div class="modal-body" style="height: auto;" >
                                            <table width="auto" class="table table-bordered table-hover table-responsive">
                                                <thead>
                                                    <tr>
                                                        <th>Job#</th>
                                                        <th>Project</th>
                                                        <th>Customer</th>
                                                        <th>Date</th>
                                                        <th>Qty</th>
                                                        <th>Sales</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                        <% olists.forEach(function(olist){ %>
                                                    <tr class="success">
                                                        <td><a href="/vieworder/<%= olist.job_ref %>" class="btn btn-primary"><%= olist.job_ref %></a></td>
                                                        <td><%= olist.project %></td>
                                                        <td><%= olist.customer %></td>
                                                        <td><%= moment(olist.order_date).format('DD-MMM-YY'); %></td>
                                                        <td><%= olist.qty %></td>
                                                        <td><%= olist.sales_per %></td>
                                                    </tr>
                                                    <% }); %>
                                                   
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                        </div>
                                    </div>
                                    <!-- /.modal-content -->
                                </div>
                                <!-- /.modal-dialog -->
                            </div>
                            <!-- /.modal -->
                        </div>
                    </div>
                    <a href="/salesorder">
                        <div class="panel-footer">
                                <span class="pull-left">View Order List</span>
                            <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                            <div class="clearfix"></div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="col-lg-4 col-md-2">
                <div class="panel panel-yellow">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-3">
                                <i class="fa fa-truck fa-fw fa-5x"></i>
                            </div>
                            <div class="col-xs-9 text-right">
                                <div class="huge"></div>
                                <div>Ready for Dispatch</div>
                            </div>
                        </div>
                    </div>
                    <a href="/dispatch">
                        <div class="panel-footer">
                               <span class="pull-left">View Dispatch List</span>
                            <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                            <div class="clearfix"></div>
                        </div>
                    </a>
                </div>
            </div>
           
            </div>
            
        </div>
</div>
