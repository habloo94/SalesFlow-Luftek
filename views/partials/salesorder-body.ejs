        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-sm-8">
                                <h4>
                                Sales Order List</h4>
                            </div>
                            <div class="col-sm-4"><h4 align="right">Net Booking Value : <%= currencyFormatter.format(sales, { code: 'INR' }); %></h4></div>
                        </div>
                            
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        
                        <table width="100%" class="table table-bordered table-hover" id="enquiry_table">
                            <thead>
                                <tr>
                                    <th>SO#</th>   
                                    <th>Job#</th>
                                    <th>Project</th>
                                    <th>Customer</th>
                                    <th>Qty</th>
                                    <th>Amount</th>
                                    <th>PO#</th>
                                    <th>PO Date</th>
                                    <th>ED Date</th>
                                    <th>Booked by</th>
                                    <th ><i class=" fa fa-gear"></i></th>
                                </tr>
                            </thead>
                            
                            <tbody class="table-bordered">
                                    <% items.forEach(function(item){ %>
                                        <tr>
                                                <td>SO/LK/<%= moment(item.po_date).format('YY'); %>/<%= item.so_num %></td>
                                                <td><%= item.job_ref %></td>
                                                <td><%= item.project %></td>
                                                <td><%= item.customer %></td>
                                                <td><%= item.qty %></td> 
                                                <td><%= currencyFormatter.format(item.amount, { code: 'INR' }); %></td>
                                                <td><%= item.po_num %></td>
                                                <td><%= moment(item.po_date).format('DD-MMM-YY'); %></td>
                                                <td><%= moment(item.deli_date).format('DD-MMM-YY'); %></td>
                                                <td><%= item.sales_per %></td>                                                <td >
                                                    <a href="/editorder/<%= item.so_num %>" class="btn btn-primary btn-circle"><i class="fa fa-edit"></i></a>
                                                    <a href="/vieworder/<%= item.so_num %>" class="btn btn-info btn-circle"><i class="fa fa-file-text"></i></a>
                                                </td>
                                            </tr>
                                            <% }); %>
                            </tbody>
                            
                        </table>
                        
                            
                        <!-- /.table-responsive -->

                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>
